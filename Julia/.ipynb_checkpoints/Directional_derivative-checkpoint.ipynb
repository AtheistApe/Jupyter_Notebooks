   "source": [
    "using ForwardDiff\n",
    "using LinearAlgebra\n",
    "\n",
    "f(x,y)=sin(x+2y)\n",
    "\n",
    "gradf = ForwardDiff.gradient(z->f(z[1],z[2]), [-2.0, -3.0])\n",
    "\n",
    "u = [cos(pi/6), sin(pi/6)]\n",
    "\n",
    "dot(gradf, u)"
   ]
